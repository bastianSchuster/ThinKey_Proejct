<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.thinkey.thinkey">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/Theme.AppCompat.NoActionBar"

        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Level1" />
        <activity android:name=".Settings" />
        <activity android:name=".Levels" />
        <activity android:name=".GameOver" android:theme="@style/AppTheme.CustomTheme"></activity>
        <activity android:name=".WinnerScreen" android:theme="@style/AppTheme.CustomTheme"></activity>


    </application>

</manifest>